{
  "private": true,
  "name": "webpack-typescript-react-starter",
  "version": "1.0.0",
  "scripts": {
    "check-types": "tsc",
    "dev": "cross-env NODE_ENV=development NODE_OPTIONS=--openssl-legacy-provider webpack-dev-server --color --progress",
    "build": "cross-env NODE_ENV=production NODE_OPTIONS=--openssl-legacy-provider webpack --color --progress",
    "start": "serve dist",
    "test": "jest"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "moduleNameMapper": {
      "^assets(.*)$": "<rootDir>/src/assets$1",
      "^@stores(.*)$": "<rootDir>/src/stores$1",
      "^@components(.*)$": "<rootDir>/src/components$1",
      "^@screens(.*)$": "<rootDir>/src/screens$1",
      "^@utils(.*)$": "<rootDir>/src/utils$1",
      "^@services(.*)$": "<rootDir>/src/services$1",
      "^@hooks(.*)$": "<rootDir>/src/hooks$1",
      "^@routes(.*)$": "<rootDir>/src/routes$1",
      "^@constants(.*)$": "<rootDir>/src/constants$1",
      "^@img(.*)$": "<rootDir>/src/img$1",
      "^types(.*)$": "<rootDir>/src/types$1",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/src/fileMock.js",
      "\\.(css|less|pcss)$": "<rootDir>/src/fileMock.js"
    }
  },
  "dependencies": {
