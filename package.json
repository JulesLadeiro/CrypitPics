    "file-loader": "^5.0.2",
    "file-saver": "^2.0.2",
    "formik": "^2.2.9",
    "html-loader": "^0.5.5",
    "js-base64": "^2.5.2",
    "jwt-decode": "^2.2.0",
    "mini-css-extract-plugin": "^0.9.0",
